{
	"compilerOptions": {
		"target": "ESNext",
    	"module": "ESNext",
		"moduleResolution": "Bundler",
    	"strict": true,
    	"esModuleInterop": true,
    	"forceConsistentCasingInFileNames": true,
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"skipLibCheck": true
	},
	"include": [
    	"src/**/*.ts"
  	]
}
