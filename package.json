{
	"name": "trace-moe-api",
	"version": "1.3.11",
	"description": "API wrapper for trace.moe",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"author": "Rodrigo Pelissier <pelirodri@icloud.com>",
	"license": "GPL-3.0",
	"repository": {
		"url": "git+https://github.com/pelirodri/trace-moe-api.git"
	},
	"homepage": "https://github.com/pelirodri/trace-moe-api#readme",
	"bugs": "https://github.com/pelirodri/trace-moe-api/issues",
	"scripts": {
		"build": "yarn clean && rollup -c rollup.config.ts --configPlugin typescript",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"clean": "rimraf dist",
		"prepare": "yarn build"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [
		"trace.moe",
		"trace-moe",
		"trace moe",
		"anime",
		"finder",
		"detector",
		"api",
		"wrapper"
	],
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"axios": "^1.7.7",
		"lodash.isundefined": "^3.0.1",
		"lodash.omitby": "^4.6.0"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "^12.1.1",
		"@types/jest": "^29.5.1",
		"@types/lodash.isundefined": "^3.0.7",
		"@types/lodash.omitby": "^4.6.7",
		"@types/mock-fs": "^4.13.1",
		"axios-mock-adapter": "^1.21.4",
		"esbuild": "^0.24.0",
		"jest": "^29.7.0",
		"mock-fs": "^5.2.0",
		"rimraf": "^5.0.1",
		"rollup": "^4.27.3",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"typescript": "^5.6.3"
	}
}
